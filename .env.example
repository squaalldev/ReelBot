# Copia este archivo a .env y añade tu API key de Google Gemini
VITE_API_KEY=tu_api_key_de_gemini_aqui
VITE_GEMINI_API_KEY=tu_api_key_de_gemini_aqui

# Configuración de Supabase (opcional, para mayor seguridad)
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co
VITE_SUPABASE_ANON_KEY=tu_anon_key_de_supabase
VITE_USE_SUPABASE=false

# Cambia a true para usar Supabase Edge Functions (más seguro)
# VITE_USE_SUPABASE=true